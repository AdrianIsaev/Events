<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_margin="5sp">
    <TextView
        android:layout_marginTop="20sp"
        android:layout_marginBottom="20sp"
        android:layout_below="@+id/textViewRegistr4"
        android:id="@+id/textViewRegistr5"
        android:layout_width="wrap_content"
        android:fontFamily="@font/eft"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        android:textColor="@color/black"
        android:text="Новый пароль:" />

    <TextView
        android:layout_marginTop="0sp"
        android:id="@+id/textViewRegistr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="51sp"
        android:fontFamily="@font/eft"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"
        android:textColor="@color/black"
        android:text="Профиль" />

    <EditText
        android:id="@+id/textViewRegistr4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewRegistr3"
        android:layout_marginStart="5sp"
        android:layout_marginTop="5sp"
        android:fontFamily="@font/eft"
        android:layout_marginEnd="5sp"
        android:layout_marginBottom="5sp"
        android:background="@drawable/edittextstyle"
        android:inputType="textEmailAddress"
        android:textColor="@color/black"
        android:textSize="24sp" />


    <androidx.appcompat.widget.AppCompatButton
        android:layout_marginTop="20sp"
        android:id="@+id/textViewRegistr7"
        android:layout_width="375dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text2"
        android:layout_marginBottom="0dp"
        android:textColor="@color/white"
        android:fontFamily="@font/eft"
        android:textSize="24sp"
        android:background="@drawable/krygkracniy"
        android:layout_centerHorizontal="true"
        android:text="ИЗМЕНИТЬ" />


    <TextView
        android:layout_marginTop="10sp"
        android:id="@+id/textViewRegistrfix"
        android:layout_below="@+id/textViewRegistr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20sp"
        android:fontFamily="@font/eft"
        android:textSize="24sp"
        android:textColor="@color/black"
        android:text="Ваше текущее имя:" />

    <EditText
        android:id="@+id/textViewRegistrfix2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewRegistrfix"
        android:background="@drawable/edittextstyle"
        android:layout_marginStart="5sp"
        android:layout_marginTop="5sp"
        android:layout_marginEnd="5sp"
        android:layout_marginBottom="5sp"
        android:inputType="text"
        android:fontFamily="@font/eft"
        android:textColor="@color/black"
        android:textSize="24sp" />

    <TextView
        android:layout_marginTop="20sp"
        android:layout_marginBottom="20sp"
        android:id="@+id/textViewRegistr3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        android:textColor="@color/black"
        android:text="Новый email:"
        android:fontFamily="@font/eft"
        android:layout_below="@+id/textViewRegistrfix2"
        />
    <EditText
        android:id="@+id/textViewRegistr6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewRegistr5"
        android:background="@drawable/edittextstyle"
        android:layout_marginStart="5sp"
        android:layout_marginTop="5sp"
        android:layout_marginEnd="5sp"
        android:layout_marginBottom="5sp"
        android:inputType="textPassword"
        android:fontFamily="@font/eft"
        android:textColor="@color/black"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_marginTop="10sp"
        android:layout_below="@+id/textViewRegistr"
        android:layout_toRightOf="@+id/textViewRegistrfix"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20sp"
        android:textSize="24sp"
        android:fontFamily="@font/eft"
        android:layout_marginLeft="10sp"
        android:textColor="@color/black"
        android:text="" />



    <TextView
        android:id="@+id/textView5"
        android:layout_marginTop="20sp"
        android:layout_below="@+id/textViewRegistrfix2"
        android:layout_toRightOf="@+id/textViewRegistr3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20sp"
        android:textSize="24sp"
        android:fontFamily="@font/eft"
        android:layout_marginLeft="10sp"
        android:textColor="@color/black"
        android:text="" />


    <TextView
        android:id="@+id/textView6"
        android:layout_marginTop="20sp"
        android:layout_below="@+id/textViewRegistr4"
        android:layout_toRightOf="@+id/textViewRegistr5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20sp"
        android:textSize="24sp"
        android:fontFamily="@font/eft"
        android:layout_marginLeft="10sp"
        android:textColor="@color/black"
        android:text="" />
    <TextView
        android:layout_marginTop="20sp"
        android:layout_marginBottom="20sp"
        android:id="@+id/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        android:fontFamily="@font/eft"
        android:textColor="@color/black"
        android:text="Мероприятий отмечено:"
        android:layout_below="@+id/textViewRegistr6"
        />
    <TextView
        android:layout_marginTop="20sp"
        android:layout_marginBottom="20sp"
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        android:fontFamily="@font/eft"
        android:textColor="@color/black"
        android:text="Мероприятий создано:"
        android:layout_below="@+id/text"
        />
    <TextView
        android:id="@+id/textView9"
        android:layout_marginTop="20sp"
        android:layout_below="@+id/textViewRegistr6"
        android:layout_toRightOf="@+id/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20sp"
        android:textSize="24sp"
        android:fontFamily="@font/eft"
        android:layout_marginLeft="10sp"
        android:textColor="@color/black"
        android:text="TextView" />
    <TextView
        android:id="@+id/textView10"
        android:layout_marginTop="20sp"
        android:layout_below="@+id/textView9"
        android:layout_toRightOf="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20sp"
        android:textSize="24sp"
        android:fontFamily="@font/eft"
        android:layout_marginLeft="10sp"
        android:textColor="@color/black"
        android:text="TextView" />
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"></LinearLayout>

</RelativeLayout>